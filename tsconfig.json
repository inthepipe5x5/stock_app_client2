{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/assets/*": ["./assets/*"],
      "@/screens/*": ["./screens/*"],
      "@/components/*": ["./components/*"],
      "@gs": ["./components/ui/*"],
      "@/lib/*": ["./lib/*"],
      "tailwind.config": ["./tailwind.config.js"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/**/*.ts",
    "constants/**/*.ts",
    "assets/**/*.ts",
    "screens/**/*.ts",
    "components/**/*.ts",
    "lib/**/*.ts"
  ]
}
